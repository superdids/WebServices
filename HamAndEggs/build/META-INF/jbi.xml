<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://j2ee.netbeans.org/wsdl/HamAndEggsBPEL/src/hamandeggs" xmlns:ns2="http://enterprise.netbeans.org/bpel/HamAndEggsBPEL/hamAndEggsBPEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>HamAndEggs</name>
            <description>Represents the Service Assembly of HamAndEggs</description>
        </identification>
        <service-unit>
            <identification>
                <name>HamAndEggs-HamAndEggsBPEL</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>HamAndEggsBPEL.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>HamAndEggs-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="hamandeggsPort" service-name="ns1:hamandeggsService"/>
                <provider endpoint-name="hamandeggsPortTypeRole_myRole" service-name="ns2:PartnerLink1"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
